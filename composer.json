{
    "name": "owenoj/pdf-password-protect",
    "description": "Laravel package to password protect pdfs",
    "keywords": [
        "owenoj",
        "pdf-password-protect",
        "pdf protection",
        "pdf password"
    ],
    "homepage": "https://github.com/owen-oj/pdf-password-protect",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Owen Jubilant",
            "email": "owen.j@terktrendz.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "mpdf/mpdf": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Owenoj\\PdfPasswordProtect\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Owenoj\\PdfPasswordProtect\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Owenoj\\PdfPasswordProtect\\PdfPasswordProtectServiceProvider"
            ],
            "aliases": {
                "PdfPasswordProtect": "PdfPasswordProtect"
            }
        }
    }
}
